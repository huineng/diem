{
  "name": "diem-spark-operator",
  "description": "Diem Spark Operator",
  "version": "3.2.2",
  "author": "guy_huinen@be.ibm.com",
  "config": {
    "spark_py_v3_local": "127.0.0.1:30500/bizops/etl-spark-py:3.1.1",
    "spark_v3_local": "127.0.0.1:30500/bizops/etl-spark:3.1.1",
    "spark_v3": "quay.io/diem/spark:spark-3.1.1",
    "spark_py_v3": "quay.io/diem/spark:spark-py-3.1.1",
    "spark_operator": "quay.io/diem/spark:spark-operator-2.0.0",
    "spark_operator_local": "127.0.0.1:30500/bizops/spark-operator:2.0.0",
    "jupytername": "127.0.0.1:30500/bizops/jupyter",
    "jupytersqlname": "127.0.0.1:30500/bizops/jupyter-sql",
    "etl-sparkprod": "us.icr.io/bizopstest/spark"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {},
  "keywords": [
    "bizops"
  ],
  "license": "ISC",
  "private": true,
  "scripts": {
    "docker:build_spark_py_v3": "docker build -t $npm_package_config_spark_py_v3 -f deploy/Dockerfile-spark-py-3.1.1 .",
    "docker:build_spark_v3": "docker build -t $npm_package_config_spark_v3 -f deploy/Dockerfile-spark-3.1.1 .",
    "docker:build-scala": "docker build -t $npm_package_config_etl-spark-scala -f deploy/Dockerfile-scala .",
    "docker:build-production": "docker build -t $npm_package_config_etl-sparkprod -f deploy/Dockerfile .",
    "docker:build-spark-operator": "docker build -t $npm_package_config_spark_operator -f deploy/Dockerfile-spark-operator .",
    "docker:push_spark_py_v3": "docker push $npm_package_config_spark_py_v3",
    "docker:push_spark_v3": "docker push $npm_package_config_spark_v3",
    "docker:push-scala": "docker push $npm_package_config_etl-spark-scala",
    "docker:push-production": "docker push $npm_package_config_etl-sparkprod",
    "docker:push-spark-operator": "docker push $npm_package_config_spark_operator",
    "kube:local-prod": "npm-run-all docker:build docker:push",
    "kube:local-prod-scala": "npm-run-all docker:build-scala docker:push-scala",
    "kube:spark-py-v3": "npm-run-all docker:build_spark_py_v3 docker:push_spark_py_v3",
    "kube:spark-v3": "npm-run-all docker:build_spark_v3 docker:push_spark_v3",
    "kube:spark-operator": "npm-run-all docker:build-spark-operator docker:push-spark-operator",
    "kube:production": "npm-run-all docker:build-production docker:push-production",
    "cpdir": "cp -r ../../packages/diem-lib/dist/  ./local/"
  }
}

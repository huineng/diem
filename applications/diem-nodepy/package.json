{
    "name": "diem-nodepy",
    "version": "4.4.0",
    "description": "ETL- Manager NodePy",
    "config": {
        "appname": "diem-nodepy",
        "appcookie": "diem-nodepy",
        "containername": "127.0.0.1:30500/bizops/nodepy",
        "containernameprod": "us.icr.io/bizopstest/nodepy"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "helmet": "^4.4.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "print-hrtime": "^2.0.0",
        "redis": "^3.0.2",
        "rimraf": "^3.0.2"
    },
    "keywords": [
        "node",
        "python"
    ],
    "devDependencies": {
        "@mydiem/diem-common": "^2.0.0",
        "@mydiem/diem-util": "^2.4.0",
        "@types/body-parser": "^1.19.0",
        "@types/core-js": "^2.5.4",
        "@types/express": "^4.17.11",
        "@types/helmet": "^4.0.0",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^14.14.31",
        "@types/redis": "^2.8.28",
        "@types/rimraf": "^3.0.0",
        "@types/webpack": "^4.41.26",
        "@types/webpack-env": "^1.16.0",
        "@typescript-eslint/eslint-plugin": "^4.16.1",
        "@typescript-eslint/parser": "^4.16.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.21.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^32.2.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-sonarjs": "^0.6.0",
        "fork-ts-checker-webpack-plugin": "^6.1.0",
        "nodemon-webpack-plugin": "^4.4.4",
        "npm-run-all": "^4.1.5",
        "pug-loader": "^2.4.0",
        "ts-loader": "^8.0.17",
        "typescript": "^4.1.5",
        "typescript-eslint": "0.0.1-alpha.0",
        "webpack": "^5.24.2",
        "webpack-cli": "^4.5.0"
    },
    "scripts": {
        "build": "./node_modules/.bin/npm-run-all lock webpack:server webpack:node",
        "start": "node --max-old-space-size=8192 ./server/server.js",
        "start:cloud": "node --max-old-space-size=2048 ./server/app.mjs",
        "start:local": "node --max-old-space-size=2048 ./server/server.mjs",
        "eslint": "eslint -c ./src/server/.eslintrc.js ./src/server/ --ext .ts",
        "docker:push": "docker push $npm_package_config_containername",
        "docker:build": "docker build -t $npm_package_config_containername -f deploy/Dockerfile .",
        "docker:build.local": "docker build -t $npm_package_config_containername -f deploy/Dockerfile.local .",
        "docker:push-prod": "docker push $npm_package_config_containernameprod",
        "docker:build-prod": "docker build -t $npm_package_config_containernameprod -f deploy/Dockerfile .",
        "kube:init-prod": "node ../../common/k8s/init-prod",
        "kube:local-prod": "./node_modules/.bin/npm-run-all build docker:build docker:push",
        "kube:local-diemlib": "./node_modules/.bin/npm-run-all cpdir build docker:build.local docker:push",
        "kube:prod": "./node_modules/.bin/npm-run-all build docker:build-prod docker:push-prod",
        "webpack:node-test": "cross-env webpackenv=node-test webpack -w --display-error-details --inline --progress --colors --mode development",
        "webpack:node": "webpackenv=node webpack --mode production",
        "webpack:server": "cross-env webpackenv=server webpack --mode production",
        "webpack:server-test": "cross-env webpackenv=server-test webpack -w --display-error-details --inline --progress --colors --mode development",
        "lock": "npm install --package-lock",
        "kube:init": "node ../../common/k8s/init",
        "cpdir": "cp -r ../../packages/diem-lib/dist/  ./local/"
    }
}
